DECLARE\040@num1\040INTEGER;
DECLARE\040@num2\040INTEGER;
\040
SET\040@num1\040=\04020;
SET\040@num2\040=\04030;
\040
IF\040(@num1\040>\040@num2)
\040\040PRINT\040'1st\040number\040is\040greater\040than\0402nd\040number.'
ELSE\040IF\040(@num2\040>\040@num1)
\040\040PRINT\040'2nd\040number\040is\040greater\040than\0401st\040number.'
ELSE\040
\040\040PRINT\040'The\040numbers\040are\040equal.';
IF\040(@num1\040>\040@num2)\040\040\040PRINT\040'1st\040number\040is\040greater\040than\0402nd\040number.'\040ELSE\040IF\040(@num2\040>\040@num1)\040\040\040PRINT\040'2nd\040number\040is\040greater\040than\0401st\040number.'\040ELSE\040\040\040\040PRINT\040'The\040numbers\040are\040equal.';
@num1;
exit
